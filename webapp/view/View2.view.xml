<mvc:View
    controllerName="com.sap.supplier.controller.View2"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns:semantic="sap.f.semantic"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:upload="sap.m.upload"
    xmlns:u="sap.ui.unified"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page2"
        title="{i18n>title}"
    >
        <content>
            <uxap:ObjectPageLayout
                id="ObjectPageLayout1"
                upperCaseAnchorBar="false"
                showAnchorBar="true"
                enableLazyLoading="true"
            >
                <uxap:sections>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection1">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection1"
                                title="About "
                                titleUppercase="false"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm1"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_IDGenLabel1"
                                            text="Company Name "
                                        />
                                        <Text
                                            id="_IDGenText1"
                                            text="{companyName}"
                                        />

                                        <Label
                                            id="_IDGenLabel2"
                                            text="Link to SAP ARIBA"
                                        />
                                        <Link
                                            id="_IDGenText2"
                                            href="https://s1-eu.ariba.com/Sourcing/Main/aw?awh=r&amp;awssk=mORR3Wq74B.rFj_C&amp;realm=744088967-T&amp;dard=1#b0"
                                            text="https://s1-eu.ariba.com/Sourcing/Main/aw?awh=r&amp;awssk=mORR3Wq74B.rFj_C&amp;realm=744088967-T&amp;dard=1#b0"
                                            target="_blank"
                                        />
                                        <!-- <Label
                                            id="_IDGenLabel02"
                                            text="Link to repository of opentext"
                                        />
                                        <Link
                                            id="_IDGenText02"
                                            text=" {openText}"
                                        /> -->

                                        <Label
                                            id="_IDGenLabe"
                                            text="Registration Status"
                                        />
                                        <Text
                                            id="_IDGenInpt3"
                                            text="{registrationStatus}"
                                        />
                                        <Label
                                            id="_IDGenLabel3"
                                            text="Registration Date"
                                        />
                                        <Text
                                            id="_IDGenText3"
                                            text="{registrationDate}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection4">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection2"
                                title="Supplier ID"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm2"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_IDGenLabel11"
                                            text="SM Vendor ID"
                                        />
                                        <Text
                                            id="_IDGenText12"
                                            text="{smVendorId}"
                                        />

                                        <Label
                                            id="_IDGenLabe2l2"
                                            text="ACM ID"
                                        />
                                        <Text
                                            id="_IDGenText122"
                                            text="{acmId}"
                                        />

                                        <Label
                                            id="lb1"
                                            text="ERP ID"
                                        />
                                        <Text
                                            id="lb2"
                                            text="{erpId}"
                                        />
                                        <Label
                                            id="lb3"
                                            text="AN ID"
                                        />
                                        <Text
                                            id="lb4"
                                            text="{anId}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection5">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="IDGenObjectPageSubSection1223"
                                title="Company profile"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm3"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <!-- <Label
                                            id="_h1"
                                            text="Company aliases"
                                        />
                                        <Text
                                            id="h2"
                                            text="{companyAlias}"
                                        />

                                        <Label
                                            id="_h3"
                                            text="business type"
                                        />
                                        <Text
                                            id="h4"
                                            text="{businessType}"
                                        />

                                        <Label
                                            id="_h5"
                                            text="revenue"
                                        />
                                        <Text
                                            id="h6"
                                            text="{revenue}"
                                        /> -->

                                        <Label
                                            id="_h7"
                                            text="address"
                                        />
                                        <Text
                                            id="h8"
                                            text="{address}"
                                        />
                                        <Label
                                            id="_h9"
                                            text="supplier legal form"
                                        />
                                        <Text
                                            id="h10"
                                            text="{legalForm}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection6">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection4"
                                title="Contacts"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm4"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_q1"
                                            text="Email"
                                        />
                                        <Text
                                            id="_q2"
                                            text="{emailId}"
                                        />

                                        <Label
                                            id="_q3"
                                            text="Phone"
                                        />
                                        <Text
                                            id="_q4"
                                            text="{phone}"
                                        />
                                        <Label
                                            id="_q5"
                                            text="Mobile "
                                        />
                                        <Text
                                            id="_q6"
                                            text="{mobile}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>

                    <uxap:ObjectPageSection id="_IDGenObjectPageSection07">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection07"
                                title="Commodity"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm07"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_Ci1"
                                            text="Name"
                                        />
                                        <Text
                                            id="_C2"
                                            text="{commodity}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection7">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection7"
                                title="Qualifications (if possible)"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm7"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_ai1"
                                            text="Category"
                                        />
                                        <Text
                                            id="_ai2"
                                            text="{category}"
                                        />

                                        <Label
                                            id="_ai3"
                                            text="Pre-Qualification Status"
                                        />
                                        <Text
                                            id="_ai4"
                                            text="{preqStatus}"
                                        />

                                        <Label
                                            id="_ai7"
                                            text="End date"
                                        />
                                        <Text
                                            id="_ai8"
                                            text="{preqExpiryDate}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection007">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection007"
                                title="Statuses"
                            >
                                <uxap:blocks>
                                    <f:SimpleForm
                                        id="supplierForm007"
                                        layout="ResponsiveGridLayout"
                                        labelSpanXL="3"
                                        labelSpanL="3"
                                        labelSpanM="3"
                                        labelSpanS="12"
                                        adjustLabelSpan="false"
                                    >
                                        <Label
                                            id="_si1"
                                            text="Ethical Due Diligence"
                                        />
                                        <Text
                                            id="_s2"
                                            text="{eddStatus}"
                                        />
                                        <Label
                                            id="_si2"
                                            text="Vendor Feedback"
                                        />
                                        <Text
                                            id="_s02"
                                            text="{vendorFeedbackStatus}"
                                        />
                                    </f:SimpleForm>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                </uxap:sections>
            </uxap:ObjectPageLayout>
        </content>
    </Page>
</mvc:View>
