<mvc:View
    controllerName="com.sap.supplier.controller.View2"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:uxap="sap.uxap"
    xmlns:core="sap.ui.core"
    xmlns:semantic="sap.f.semantic"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:upload="sap.m.upload"
    xmlns:u="sap.ui.unified"
    displayBlock="true"
    xmlns="sap.m"
>
    <Page
        id="page"
        title="{i18n>title}"
    >
        <content>
            <uxap:ObjectPageLayout
                id="ObjectPageLayout1"
                upperCaseAnchorBar="false"
                showAnchorBar="true"
                enableLazyLoading="true"
            >
                <uxap:sections>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection1">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection1"
                                title="About "
                                titleUppercase="false"
                            >
                                <uxap:blocks>
                                    <Panel
                                        id="_IDGenPanel1"
                                        expandable="true"
                                        width="auto"
                                    >
                                        <f:SimpleForm
                                            id="supplierForm1"
                                            layout="ResponsiveGridLayout"
                                            labelSpanXL="3"
                                            labelSpanL="3"
                                            labelSpanM="3"
                                            labelSpanS="12"
                                            adjustLabelSpan="false"
                                        >
                                            <Label
                                                id="_IDGenLabel1"
                                                text="Company Name "
                                                required="true"
                                               
                                            />
                                            <Input
                                                id="_IDGenInput1"
                                                value="dubai Enterprises"
                                                editable="false"
                                            />

                                            <Label
                                                id="_IDGenLabel2"
                                                text="Link to SAP ARIBA"
                                                required="true"
                                            />
                                            <Link
                                                id="_IDGenInput2"
                                                text="Link to SAP ARIBA"

                                    
                                               
                                            />

                                            <Label
                                                id="_IDGenLabe"
                                                text="Registration Status"
                                                required="true"
                                            />
                                            <Input
                                                id="_IDGenInpt3"
                                                value="Registered"
                                                editable="false"
                                            />
                                            <Label
                                                id="_IDGenLabel3"
                                                text="Registration Date"
                                                required="true"
                                            />
                                            <DatePicker
                                                id="_IDGenInput3"
                                                value="08-05-2024"
                                                editable="false"
                                                
                                            />
                                        </f:SimpleForm>
                                    </Panel>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection4">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection2"
                                title="Supplier ID"
                            >
                                <uxap:blocks>
                                    <Panel
                                        id="_IDGenPanel12"
                                        expandable="true"
                                        width="auto"
                                    >
                                        <f:SimpleForm
                                            id="supplierForm2"
                                            layout="ResponsiveGridLayout"
                                            labelSpanXL="3"
                                            labelSpanL="3"
                                            labelSpanM="3"
                                            labelSpanS="12"
                                            adjustLabelSpan="false"
                                        >
                                            <Label
                                                id="_IDGenLabel11"
                                                text="SM Vendor ID"
                                                required="true"
                                            />
                                            <Input
                                                id="_IDGenInput12"
                                                value="SMVEN1001"
                                                editable="false"
                                            />

                                            <Label
                                                id="_IDGenLabe2l2"
                                                text="ACM ID"
                                                required="true"
                                            />
                                            <Input
                                                id="_IDGenInput122"
                                                value="ACM20001"
                                                editable="false"
                                            />

                                            <Label
                                                id="lb1"
                                                text="ERP ID"
                                                required="true"
                                            />
                                            <Input
                                                id="lb2"
                                                value="ERP10101"
                                                editable="false"
                                            />
                                             <Label
                                                id="lb3"
                                                text="AN ID"
                                                required="true"
                                            />
                                            <Input
                                                id="lb4"
                                                value="AN9012"
                                                editable="false"
                                            />
                                        </f:SimpleForm>
                                    </Panel>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection5">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="IDGenObjectPageSubSection1223"
                                title="Company profile (network)"
                            >
                                <uxap:blocks>
                                    <Panel
                                        id="_IDGenPanel123"
                                        expandable="true"
                                        width="auto"
                                    >
                                        <f:SimpleForm
                                            id="supplierForm3"
                                            layout="ResponsiveGridLayout"
                                            labelSpanXL="3"
                                            labelSpanL="3"
                                            labelSpanM="3"
                                            labelSpanS="12"
                                            adjustLabelSpan="false"
                                        >
                                            <Label
                                                id="_h1"
                                                text="Company aliases"
                                                required="true"
                                            />
                                            <Input
                                                id="h2"
                                                value=""
                                            />

                                            <Label
                                                id="_h3"
                                                text="business type"
                                                required="true"
                                            />
                                            <Input
                                                id="h4"
                                                value=""
                                            />

                                            <Label
                                                id="_h5"
                                                text="revenue"
                                                required="true"
                                            />
                                            <Input
                                                id="h6"
                                                value=""
                                            />

                                            <Label
                                                id="_h7"
                                                text="address"
                                                required="true"
                                            />
                                            <Input
                                                id="h8"
                                                value=""
                                            />
                                            <Label
                                                id="_h9"
                                                text="supplier legal form"
                                                required="true"
                                            />
                                            <Input
                                                id="h10"
                                                value=""
                                            />
                                        </f:SimpleForm>
                                    </Panel>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection6">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection4"
                                title="Contacts"
                            >
                                <uxap:blocks>
                                    <Panel
                                        id="_IDGenPanel1234"
                                        expandable="true"
                                        width="auto"
                                    >
                                        <f:SimpleForm
                                            id="supplierForm4"
                                            layout="ResponsiveGridLayout"
                                            labelSpanXL="3"
                                            labelSpanL="3"
                                            labelSpanM="3"
                                            labelSpanS="12"
                                            adjustLabelSpan="false"
                                        >
                                            <Label
                                                id="_q1"
                                                text="Email"
                                                required="true"
                                            />
                                            <Input
                                                id="_q2"
                                                value=""
                                            />

                                            <Label
                                                id="_q3"
                                                text="Phone"
                                                required="true"
                                            />
                                            <Input
                                                id="_q4"
                                                value=""
                                            />
                                            <Label
                                                id="_q5"
                                                text="Mobile "
                                                required="true"
                                            />
                                            <Input
                                                id="_q6"
                                                value=""
                                            />
                                        </f:SimpleForm>
                                    </Panel>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>

                    <uxap:ObjectPageSection id="_IDGenObjectPageSection8">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection5"
                                title="Attachments"
                            >
                                <uxap:blocks>
                                    <Panel
                                        id="_IDGenPanel0"
                                        expandable="true"
                                        width="auto"
                                    >
                                        <upload:UploadSetwithTable
                                            id="UploadSetTable"
                                            sticky="ColumnHeaders,HeaderToolbar"
                                            width="auto"
                                            fixedLayout="false"
                                            inset="true"
                                            autoPopinMode="true"
                                            hiddenInPopin="None"
                                            uploadCompleted="onUploadCompleted"
                                            beforeInitiatingItemUpload="onBeforeInitiatingItemUpload"
                                            itemValidationHandler=".itemValidationCallback"
                                            beforeUploadStarts="onBeforeUploadStarts"
                                            selectionChange="onSelectionChange"
                                            itemRenamed="onDocumentRenamedSuccess"
                                            mediaTypes="text/plain,application/msword,image/png"
                                            maxFileNameLength="60"
                                            uploadUrl="/uploadFiles"
                                            multiple="true"
                                            items="{sModel>/}"
                                            mode="MultiSelect"
                                        >
                                            <upload:headerToolbar>
                                                <OverflowToolbar
                                                    id="_IDGenOverflowToolbar1"
                                                >
                                                    <Title
                                                        id="_IDGenTitle1"
                                                        level="H2"
                                                    />
                                                    <ToolbarSpacer
                                                        id="_IDGenToolbarSpacer3"
                                                    />
                                                    <SearchField
                                                        id="_IDGenSearchField2"
                                                        width="20%"
                                                        liveChange=".onSearch"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData3"
                                                                priority="NeverOverflow"
                                                            />
                                                        </layoutData>
                                                    </SearchField>
                                                    <ToolbarSeparator
                                                        id="_IDGenToolbarSeparator1"
                                                    />
                                                    <upload:ActionsPlaceholder
                                                        id="_IDGenActionsPlaceholder1"
                                                        placeholderFor="UploadButtonPlaceholder"
                                                    />
                                                    <Button
                                                        id="downloadSelectedButton"
                                                        text="Download"
                                                        enabled="false"
                                                        press="onDownloadFiles"
                                                    />
                                                    <MenuButton
                                                        id="_IDGenMenuButton1"
                                                        text="Add"
                                                    >
                                                        <menu>
                                                            <Menu
                                                                id="_IDGenMenu1"
                                                            >
                                                                <MenuItem
                                                                    id="_IDGenMenuItem1"
                                                                    text="URL"
                                                                    press="openAddOrEditDialog"
                                                                />
                                                                <MenuItem
                                                                    id="_IDGenMenuItem2"
                                                                    text="Document Without File"
                                                                    press=".openDocumentWithoutFileDialog"
                                                                />
                                                            </Menu>
                                                        </menu>
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData4"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </MenuButton>
                                                    <Button
                                                        id="removeDocumentButton"
                                                        text="Remove"
                                                        press=".onRemoveButtonFromMenuDocumentHandler"
                                                        enabled="false"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData5"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </Button>
                                                    <Button
                                                        id="renameButton"
                                                        enabled="false"
                                                        press="onRenameDocument"
                                                        text="Rename"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData6"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </Button>
                                                    <Button
                                                        id="editUrlButton"
                                                        text="Edit URL"
                                                        enabled="false"
                                                        press="onEditUrl"
                                                    >
                                                        <layoutData>
                                                            <OverflowToolbarLayoutData
                                                                id="_IDGenOverflowToolbarLayoutData7"
                                                                priority="AlwaysOverflow"
                                                            />
                                                        </layoutData>
                                                    </Button>
                                                </OverflowToolbar>
                                            </upload:headerToolbar>
                                            <upload:columns>
                                                <Column
                                                    id="filName"
                                                    importance="High"
                                                >
                                                    <header>
                                                        <Label
                                                            id="_IDGenLa5"
                                                            text="file Name"
                                                        />
                                                    </header>
                                                </Column>
                                                <!-- <Column
                                                                id="fileName"
                                                                importance="High"
                                                            >
                                                                <header>
                                                                    <Label
                                                                        id="_IDGenLabel6"
                                                                        text="Name"
                                                                    />
                                                                </header>
                                                            </Column> -->
                                                <Column id="id">
                                                    <header>
                                                        <Label
                                                            id="_IDGenLabel7"
                                                            text="Type"
                                                        />
                                                    </header>
                                                </Column>

                                                <Column
                                                    id="actionButton"
                                                    importance="High"
                                                />
                                            </upload:columns>
                                            <upload:items>
                                                <upload:UploadSetwithTableItem
                                                    id="_IDGenUploadSetwithTableItem1"
                                                    fileName="{sModel>filename}"
                                                    mediaType="{sModel>mediaType}"
                                                >
                                                    <!-- Cells with cell template defining content for each column (cells and columns work with 1:1 mapping) -->
                                                    <upload:cells>
                                                        <HBox id="_IDGenHBox2">
                                                            <core:Icon
                                                                id="_IDGenIcon1"
                                                                src="{sModel>mediaType}"
                                                                color="white"
                                                                class="sapMUSTItemImage sapMUSTItemIcon"
                                                            />
                                                            <VBox
                                                                id="_IDGenVBox1"
                                                                class="sapUiTinyMargin sapUiSmallMarginBegin"
                                                            >
                                                                <Link
                                                                    id="_IDGenLink3"
                                                                    text="{sModel>filename}"
                                                                    press="openPreview"
                                                                    class="sapUiTinyMarginBottom"
                                                                />
                                                                <Text
                                                                    id="_IDGenText10"
                                                                    class="sapUiTinyMarginTop"
                                                                />
                                                            </VBox>
                                                        </HBox>
                                                        <!-- <Text
                                                                        id="_IDGen"
                                                                        text="{sModel>filename}"
                                                                    /> -->

                                                        <!-- <Text
                                                                        id="_IDGenText11"
                                                                        text="{sModel>name}"
                                                                    /> -->
                                                        <Text
                                                            id="_IDGenLink2"
                                                            text="{sModel>type}"
                                                        />

                                                        <Button
                                                            id="_IDGenButton2"
                                                            class="sapUiTinyMarginBegin"
                                                            type="Transparent"
                                                            icon="sap-icon://decline"
                                                            press="onRemoveHandler"
                                                        />
                                                    </upload:cells>
                                                </upload:UploadSetwithTableItem>
                                            </upload:items>
                                        </upload:UploadSetwithTable>
                                    </Panel>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                    <uxap:ObjectPageSection id="_IDGenObjectPageSection7">
                        <uxap:subSections>
                            <uxap:ObjectPageSubSection
                                id="_IDGenObjectPageSubSection7"
                                title="Qualifications"
                            >
                                <uxap:blocks>
                                    <Panel
                                        id="_IDGenPanel101"
                                        expandable="true"
                                        width="auto"
                                    >
                                        <f:SimpleForm
                                            id="supplierForm7"
                                            layout="ResponsiveGridLayout"
                                            labelSpanXL="3"
                                            labelSpanL="3"
                                            labelSpanM="3"
                                            labelSpanS="12"
                                            adjustLabelSpan="false"
                                        >
                                            <Label
                                                id="_ai1"
                                                text="Category"
                                                required="true"
                                            />
                                            <Input
                                                id="_ai2"
                                                value=""
                                            />

                                            <Label
                                                id="_ai3"
                                                text="Status"
                                                required="true"
                                            />
                                            <Input
                                                id="_ai4"
                                                value=""
                                            />
                                            <Label
                                                id="_ai5"
                                                text="Owner"
                                                required="true"
                                            />
                                            <Input
                                                id="_ai6"
                                                value=""
                                            />
                                            <Label
                                                id="_ai7"
                                                text="End date"
                                                required="true"
                                            />
                                            <DatePicker
                                                id="_ai8"
                                                value=""
                                            />
                                            <Label
                                                id="_ai9"
                                                text="Action"
                                                required="true"
                                            />
                                            <Input
                                                id="_ai10"
                                                value=""
                                            />
                                        </f:SimpleForm>
                                    </Panel>
                                </uxap:blocks>
                            </uxap:ObjectPageSubSection>
                        </uxap:subSections>
                    </uxap:ObjectPageSection>
                </uxap:sections>
            </uxap:ObjectPageLayout>
        </content>
    </Page>
</mvc:View>
